<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xu.eureka.client.mapper.TablesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xu.eureka.client.entity.Tables">
        <id column="tid" property="tid" />
        <result column="fid" property="fid" />
        <result column="tag" property="tag" />
        <result column="row" property="row" />
        <result column="col" property="col" />
        <result column="type" property="type" />
        <result column="name" property="name" />
        <result column="detail" property="detail" />
        <result column="useable" property="useable" />
        <result column="node_first" property="nodeFirst" />
        <result column="node_index" property="nodeIndex" />
        <result column="node_create_date" property="nodeCreateDate" />
        <result column="node_update_date" property="nodeUpdateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        tid, fid, tag, row, col, type, name, detail, useable, node_first, node_index, node_create_date, node_update_date
    </sql>

</mapper>
